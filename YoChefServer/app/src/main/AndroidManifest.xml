<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kj.satijas.yochefserver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerActivity" />
        <activity android:name=".Order_Management" />

        <service
            android:name=".Service.ListenOrders"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".OrderDetail" />
        <activity android:name=".Functionality" />
        <activity android:name=".Income" />
        <activity android:name=".AddNewChef"></activity>
    </application>

</manifest>